r\n                <div class=\"layui-form-item\">\r\n                    <label class=\"layui-form-label\">电话</label>\r\n                    <div class=\"layui-input-block\">\r\n                        <input type=\"text\"  name=\"phone\" value=\"\" lay-verify=\"title\" autocomplete=\"off\" placeholder=\"请输入标题\" class=\"layui-input\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"layui-form-item\">\r\n                    <label class=\"layui-form-label\">性别</label>\r\n                    <div class=\"layui-input-block\">\r\n                        <input type=\"radio\" name=\"sex\" value=\"0\" title=\"男\">\r\n                        <input type=\"radio\" name=\"sex\" value=\"1\" title=\"女\" checked>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"bottom\">\r\n        <div class=\"button-container\">\r\n            <button type=\"submit\" class=\"layui-btn layui-btn-normal layui-btn-sm\" lay-submit=\"\" lay-filter=\"user-save\">\r\n                <i class=\"layui-icon layui-icon-ok\"></i>\r\n                提交\r\n            </button>\r\n            <button type=\"reset\" class=\"layui-btn layui-btn-primary layui-btn-sm\">\r\n                <i class=\"layui-icon layui-icon-refresh\"></i>\r\n                重置\r\n            </button>\r\n        </div>\r\n    </div>\r\n</form>\r\n<script th:src=\"@{/static/component/pear/layui/layui.js}\"></script>\r\n<script th:src=\"@{/static/component/pear/pear.js}\"></script>\r\n<script>\r\nlayui.use(['form','jquery'],function(){\r\n    let form = layui.form;\r\n    let $ = layui.jquery;\r\n\r\n    form.on('submit(user-save)', function(data){\r\n        let roleIds = \"\";\r\n        $('input[type=checkbox]:checked').each(function() {\r\n            roleIds += $(this).val()+\",\";\r\n        });\r\n        roleIds = roleIds.substr(0,roleIds.length-1);\r\n        data.field.roleIds = roleIds;\r\n        //提